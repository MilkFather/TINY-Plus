/** This is a test program that calculates the sum of 1, 2, ... 100 **/
INT recursiveAdd(INT start, INT end)
BEGIN
    IF (start > end) 
    BEGIN
        RETURN 0;
    END 
    ELSE
    BEGIN
        RETURN start + recursiveAdd(start + 1, end);
    END
END

INT iterativeAdd(INT start, INT end)
BEGIN
    INT res;
    res := 0;
    INT i;
    i := start;
    WHILE (i <= end)
    BEGIN
        res := res + i;
        i := i + 1;
    END
    RETURN res;
END

INT MAIN main()
BEGIN
    INT result1;
    result1 := recursiveAdd(1, 100);
    WRITE(iterativeAdd(1, 100), "output.txt");
END